{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-config-airbnb"
    ],
    "jsRules": {},
    "rules": {
        "variable-name": [
            true,
            "ban-keywords",
            "check-format",
            "allow-pascal-case"
        ],
        "object-literal-sort-keys": false,
        "ordered-imports": false,
        "import-name": false,
        "max-line-length": [
            true,
            120
        ],
        "no-this-assignment": false,
        "member-access": [
            false,
            "check-accessor"
        ]
    },
    "rulesDirectory": []
}