{
  "name": "react-balloon.css",
  "version": "2.0.0",
  "description": "A react styled-components implementation of kazzkiq's balloon.css",
  "main": "build/index.js",
  "scripts": {
    "preversion": "yarn test",
    "postversion": "git push && git push --tags && npm publish",
    "prepare": "yarn build",
    "compile": "projectz compile",
    "build": "env-cmd prod webpack --progress --profile --bail",
    "dev": "env-cmd dev webpack --progress --profile --watch",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "homepage": "http://react-balloon.css-demo.surge.sh",
  "author": "2017+ Reme Le Hane <reme.lehane@gmail.com> (http://www.remelehane.me)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/RemeJuan/react-balloon.css"
  },
  "bugs": {
    "url": "https://github.com/RemeJuan/react-balloon.css/issues"
  },
  "keywords": [
    "react",
    "reactjs",
    "tooltip",
    "styled component",
    "styled tooltip"
  ],
  "devDependencies": {
    "@types/react": "^16.3.17",
    "@types/react-dom": "^16.0.6",
    "@types/react-router-dom": "^4.2.7",
    "awesome-typescript-loader": "^5.0.0",
    "env-cmd": "^8.0.2",
    "jest": "^20.0.4",
    "path": "^0.12.7",
    "prop-types": "^15.5.10",
    "react": "16",
    "react-dom": "16",
    "react-highlight": "^0.10.0",
    "react-router-dom": "^4.2.2",
    "react-test-renderer": "16",
    "source-map-loader": "^0.2.3",
    "styled-components": "^2.1.2",
    "styled-tools": "^0.1.4",
    "surge": "^0.19.0",
    "tslint": "^5.10.0",
    "tslint-config-airbnb": "^5.9.2",
    "typescript": "^2.9.1",
    "typescript-styled-plugin": "^0.6.3",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.8"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|scss|sass)$": "<rootDir>/__mocks__/styleMocks.js"
    }
  },
  "maintainers": [
    "Reme Le Hane (reme.lehane@gmail.com) (https://www.remelehane.me)"
  ],
  "contributors": [
    "Reme Le Hane <reme.lehane@gmail.com> (http://www.remelehane.me)"
  ]
}
